    "@types/cache-manager": "^3.4.3",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "helmet": "^5.0.2",
